<h2>New Restaurant details</h2>
<!-- <p>Find me in app/views/restaurants/new.html.erb</p> -->
<%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :category %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.button :submit, class: "btn btn-primary" %>
<% end %>

